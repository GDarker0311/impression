{
  "name": "impression.js",
  "version": "0.0.0",
  "description": "impression",
  "main": "./dist/index.js",
  "scripts": {
    "build": "npm run build:dist && npm run build:bundle",
    "build:dist": "babel ./source --out-dir ./dist",
    "build:bundle": "webpack",
    "test:test": "phantomjs ./node_modules/mocha-phantomjs-core/mocha-phantomjs-core.js test/index.html spec '{\"hooks\": \"mocha-phantomjs-istanbul\", \"coverageFile\": \".coverage.json\"}'",
    "test:local": "istanbul cover _mocha -- -R spec --require babel-register",
    "test": "npm run test:local && codecov"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vivaxy/impression.git"
  },
  "keywords": [
    "impression"
  ],
  "author": "vivaxy",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/vivaxy/impression/issues"
  },
  "homepage": "https://github.com/vivaxy/impression#readme",
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-loader": "^6.2.7",
    "babel-plugin-syntax-async-functions": "^6.13.0",
    "babel-plugin-transform-class-properties": "^6.18.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-function-bind": "^6.8.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-0": "^6.16.0",
    "babel-register": "^6.18.0",
    "chai": "^3.5.0",
    "codecov": "^1.0.1",
    "istanbul": "^1.1.0-alpha.1",
    "mocha": "^3.1.2",
    "mocha-phantomjs-istanbul": "0.0.2",
    "phantomjs": "^2.1.7",
    "webpack": "^1.13.3"
  },
  "dependencies": {
    "events": "^1.1.1"
  }
}
